<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/logout_yes_bt"
        android:layout_width="match_parent"
        android:layout_height="@dimen/diolg_bottom_height"
        android:background="@drawable/selector_item_bg"
        android:gravity="center"
        android:text="保存图片"
        android:textColor="@color/color212121"
        android:textSize="@dimen/dm028" />

    <include layout="@layout/layout_line_10px_f5" />

    <TextView
        android:id="@+id/logout_cancel_bt"
        android:layout_width="match_parent"
        android:layout_height="@dimen/diolg_bottom_height"
        android:background="@drawable/selector_item_bg"
        android:gravity="center"
        android:text="取消"
        android:textColor="@color/color212121"
        android:textSize="@dimen/dm026" />
</LinearLayout>