<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:showIn="@layout/activity_wage_month_add">

    <include layout="@layout/layout_line_height_f5" />

    <TextView
        style="@style/px26_mainColor"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:background="@drawable/shape_line_divider_bottom"
        android:drawableLeft="@drawable/shape_drawing_f5f5f5_7px_22px"
        android:drawablePadding="@dimen/dm015"
        android:gravity="center_vertical"
        android:onClick="onProjsMonthDetailClick"
        android:paddingLeft="@dimen/main_margin_left_x"
        android:text="项目月度工资明细" />

    <TextView
        style="@style/px26_mainColor"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:background="@drawable/shape_line_divider_bottom"
        android:drawableLeft="@drawable/shape_drawing_f5f5f5_7px_22px"
        android:drawablePadding="@dimen/dm015"
        android:gravity="center_vertical"
        android:onClick="onEmplyeeMonthDetailClick"
        android:paddingLeft="@dimen/main_margin_left_x"
        android:text="员工月度工资明细" />
</LinearLayout>