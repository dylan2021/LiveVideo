<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="mobile_common_checks_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/mobile_common_transparent</item>
        <item name="android:windowAnimationStyle">@style/mobile_common_dialog_anima</item>
    </style>
    <!--Dialog 布局使用match_parent属性时，使用此样式-->

    <style name="mobile_common_TextAppearance.22sp_black_bold" parent="@android:style/TextAppearance">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/c2</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- 通用标题-->
    <style name="mobile_common_common_title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/mobile_common_common_title_height</item>
        <item name="android:background">@color/c43</item>
    </style>

    <style name="mobile_common_cam_text_edit">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/c5</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="mobile_common_cam_text_edit.enabled">
        <item name="android:textColor">@color/mobile_common_text_edit_enable_selector</item>
    </style>

    <style name="mobile_common_custom_dialog" parent="@android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <!-- Dialog 样式 背景透明  无标题 -->


</resources>